{
  "name": "@coderscamp/ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "start-storybook -p 6006",
    "build": "build-storybook",
    "start": "serve storybook-static",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "generate:theme": "chakra-cli tokens \"src/theme/overwrites/index.ts\"",
    "generate:icons": "rimraf \"src/icons\" && svgr --config-file svgr-icons.config.js -d src/icons icons",
    "generate:svg": "rimraf \"src/svg\" && svgr --config-file svgr-svg.config.js -d src/svg svg",
    "generate": "concurrently \"yarn generate:theme\" \"yarn generate:icons\" \"yarn generate:svg\""
  },
  "peerDependencies": {
    "react": "17.0.2"
  },
  "dependencies": {
    "@chakra-ui/react": "1.8.1",
    "@chakra-ui/theme-tools": "1.3.1",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@types/react-router-dom": "5.3.3",
    "@types/storybook-react-router": "1.0.1",
    "framer-motion": "6.2.3",
    "react-router-dom": "6.2.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "1.7.0",
    "@storybook/addon-actions": "6.4.16",
    "@storybook/addon-docs": "6.4.16",
    "@storybook/addon-essentials": "6.4.16",
    "@storybook/addon-links": "6.4.16",
    "@storybook/react": "6.4.16",
    "@svgr/cli": "6.2.1",
    "@testing-library/dom": "8.11.3",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "react": "17.0.2",
    "storybook-react-router": "1.0.8"
  }
}
